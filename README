## run diff
php ./bin/diff.php -t json -f 'resource/file1.json resource/file2.json resource/file3.json' -o outFile

## run unitest
cd ./unitest
phpunit UtilsTest.php 
phpunit JsonFileReaderTest.php 
phpunit XmlFileReaderTest.php

